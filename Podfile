project 'ThreeLogin.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ThreeLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ThreeLogin
  pod 'Fabric'
  pod 'TwitterKit'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'Google/SignIn', :git => 'https://github.com/mlibai/Google.git' #, :tag => '0.7.0'

  target 'ThreeLoginTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ThreeLoginUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
